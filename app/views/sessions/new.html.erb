<div class="container">
  <%= form_tag sessions_path, class: "well" do %>
   <h1>Sign In</h1>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
    <%= label_tag :password %>
      <%= password_field_tag :password %>
  <div>
    <p>New user? <%= link_to "Sign up now!", signup_path %><p>
    <%= submit_tag "Sign in", class: "btn btn-primary btn-large" %>
  </div>
    <% end %>
</div>